
@{
    ViewData["Title"] = "CreateFaculty";
}
    
    <div class="create-university">
        <div class="container">
            <h1 style="text-align:center">Create Faculty </h1>
            <form name="my-form" autocomplete="off" action="/Administrator/CreateFaculty" method="post">

                <div class="form-group" style="text-align:center">
                    <label for="FacultyName">Faculty name</label>
                    <input type="text" class="form-control" name="FacultyName" style="width: 420px;" placeholder="Type full faculty name" required>

                </div>

                <div class="form-group" style="text-align:center">
                    <label for="Address">Address</label>
                    <input type="text" class="form-control" name="Address" style="width: 420px;" placeholder="Type full address of faculty " required>
                </div>


                <button type="submit" class="btn btn-primary">Submit</button>

            </form>
        </div>
    </div>