@model List<Application.DTOs.Administrator.UserRating>
@{
    ViewData["Title"] = "GetRating";
}

<h1 style="text-align:center">Rating</h1>

<table class="content-table table table-hover">
    <thead>
        <tr>
            <th scope="col" style="text-align:center">First Name</th>
            <th scope="col" style="text-align:center">Last Name</th>
            <th scope="col" style="text-align:center">Mark</th>            
        </tr>
    </thead>
    @foreach (var application in Model)
     {
         <tr style="text-align:center">
            <td>
                <h3>
                    @application.FirstName
                </h3>
            </td>
            <td>
                <h3>
                    @application.LastName
                </h3>
            </td>
            <td>
                <h3>
                    @application.AverageMark
                </h3>
            </td>
        </tr>
     }
</table>

